
@{
    ViewBag.Title = "Add_Coffee";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<br />
<br />
<h2>Add_Coffee</h2>
@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.Label("select coffee name", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">

            @Html.DropDownList("Coffee_ID", null, htmlAttributes: new { @class = "input", onChange = "getServPrice()", @id = "Serv_Id1" })
        </div>
    </div>
    <div class="form-group">
        @Html.Label("select Addition Type", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">

            @Html.DropDownList("Addit_ID", null, htmlAttributes: new { @class = "input", onChange = "getServPrice()", @id = "Addit_Id1" })
        </div>

    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
}